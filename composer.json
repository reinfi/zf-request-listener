{
    "name": "reinfi/zf-request-listener",
    "type": "library",
    "description": "A Zend Framework 2 module for loading listeners only for http or console.",
    "keywords": [
        "zend",
        "listener",
        "console",
        "http"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Reinfi"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laminas/laminas-modulemanager": "~2.7",
        "laminas/laminas-http": "~2.6",
        "laminas/laminas-console": "~2.6",
        "laminas/laminas-eventmanager": "~2.6 | ~3.2",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Reinfi\\RequestListener\\": "src/"
        }
    }
}
