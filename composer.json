{
    "name": "reinfi/zf-request-listener",
    "type": "library",
    "description": "A laminas module for loading listeners only for http or console.",
    "keywords": [
        "zend",
        "listener",
        "console",
        "http"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Reinfi"
        }
    ],
    "suggest": {
        "laminas/laminas-http": "If you want to enable http request listener"
    },
    "require": {
        "php": ">=7.4 | >= 8.0",
        "laminas/laminas-modulemanager": "~2.7",
        "laminas/laminas-eventmanager": "~2.6 | ~3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laminas/laminas-console": "^2.8",
        "laminas/laminas-http": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Reinfi\\RequestListener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reinfi\\RequestListener\\Test\\": "test/"
        }
    }
}
